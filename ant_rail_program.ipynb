    "def countReturn(N,A):\n",
    "    count=0\n",
    "    position=0\n",
    "    for move in A:\n",
    "        position+=move\n",
    "        if position==0:\n",
    "            count+=1\n",
    "    return count\n",
    "N=int(input().strip())\n",
    "A=[1,-1,1,-1,1]\n",
    "\n",
    "output=countReturn(N,A)\n",
    "print(output)"
   ]
